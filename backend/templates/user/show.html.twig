{% extends 'base.html.twig' %}

{% block title %}User
{% endblock %}

{% block body %}
	<h1>{{ user.firstName }}profile</h1>

	<table class="table">
		<tbody>
			
			<tr>
				<th>FirstName</th>
				<td>{{ user.firstName }}</td>
			</tr>
			<tr>
				<th>LastName</th>
				<td>{{ user.lastName }}</td>
			</tr>
			<tr>
				<th>Telephone</th>
				<td>{{ user.telephone }}</td>
			</tr>
			<tr>
				<th>Email</th>
				<td>{{ user.email }}</td>
			</tr>
			<tr>
				<th>Roles</th>
				<td>{{ user.roles ? user.roles|json_encode : '' }}</td>
			</tr>
			
			 
		</tbody>
	</table>

	<a class="btn btn-secondary" href="{{ path('app_user_index') }}">back to list</a>


{% endblock %}
